[core]
  editor = vi
  excludesfile = ~/.gitignore_global
[color]
  ui = auto
  branch = auto
  diff = auto
  interactive = auto
  status = auto
[alias]
  st = status -sb
  ci = commit
  br = branch
  co = checkout
  df = diff
  lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
  graph = log --graph --decorate --oneline
  who = shortlog -s --
  wdf = diff --color-words
