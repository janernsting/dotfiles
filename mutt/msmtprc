defaults
logfile ~/.msmtp.log

account Google
host smtp.googlemail.com
port 587
from "jan.ernsting@gmail.com"
tls on
tls_starttls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
auth on
user "jan.ernsting@gmail.com"
passwordeval ~/.mutt/getsecret.py gmail-pw

account WI
host localhost
port 1025
protocol smtp
from "jan.ernsting@wi.uni-muenster.de"
auth plain
user "j_erns01"
passwordeval ~/.mutt/getsecret.py uni

account default: Google
